{
  "name": "solar-terminal",
  "version": "1.0.0",
  "description": "Panel monitoring dashboard",
  "main": "index.js",
  "scripts": {
    "transpile": "npx babel ./ --out-dir dist --ignore '/**/*.spec.js','/**/*.test.js','/**/*.jsx','./dist/','./node_modules/','./.next/','./__tests__/','./pages/','./jest.config.js'",
    "lint": "eslint . --ext .jsx,.js,.tsx,.ts",
    "lint-staged": "lint-staged",
    "dev": "next",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "validate": "npm run lint && npm run test",
    "postinstall": "npx husky uninstall && npx husky install && npx husky add .husky/pre-commit \"node_modules/.bin/lint-staged && npm run test && npm run build\""
  },
  "author": "Amit Kumar",
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^5.28.6",
    "axios": "^1.6.8",
    "framer-motion": "^11.0.20",
    "moment": "^2.30.1",
    "next": "^14.1.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-regenerator": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.69",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ]
  }
}
